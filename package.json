{
  "name": "lws-test-suite",
  "version": "0.0.2",
  "description": "W3C Linked Web Storage Protocol conformance test suite",
  "type": "module",
  "main": "lib/runner.js",
  "bin": {
    "lws-test": "./bin/lws-test.js"
  },
  "scripts": {
    "test": "node bin/lws-test.js",
    "test:lws-server": "node bin/lws-test.js --subject lws-server",
    "test:jss": "node bin/lws-test.js --subject jss",
    "test:must": "node bin/lws-test.js --level MUST",
    "report": "node lib/report-viewer.js"
  },
  "keywords": [
    "lws",
    "linked-web-storage",
    "w3c",
    "conformance",
    "test-suite",
    "protocol",
    "testing",
    "earl"
  ],
  "author": "W3C LWS Working Group",
  "license": "W3C-20150513",
  "repository": {
    "type": "git",
    "url": "https://github.com/linkedwebstorage/test-suite"
  },
  "bugs": {
    "url": "https://github.com/linkedwebstorage/test-suite/issues"
  },
  "homepage": "https://github.com/linkedwebstorage/test-suite#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "n3": "^1.17.2",
    "commander": "^11.1.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "lws-server": "^0.0.2"
  }
}
