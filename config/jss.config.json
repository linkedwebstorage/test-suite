{
  "name": "JavaScriptSolidServer",
  "version": "0.0.77",
  "homepage": "https://github.com/JavaScriptSolidServer/JavaScriptSolidServer",
  "type": "managed",
  "server": {
    "command": "node",
    "args": ["/home/melvin/remote/github.com/JavaScriptSolidServer/JavaScriptSolidServer/bin/jss.js", "start", "--lws-mode", "--port", "3127", "--root", "./test-data-jss", "--no-multiuser"],
    "env": {
      "TOKEN_SECRET": "test-secret-for-testing-only"
    },
    "startupTimeout": 15000,
    "healthCheck": {
      "url": "http://localhost:3127/",
      "expectedStatus": 401
    }
  },
  "baseUrl": "http://localhost:3127",
  "authentication": null,
  "features": {
    "authentication": false,
    "authorization": false,
    "notifications": false,
    "sparql": false
  },
  "cleanup": {
    "dataDirectory": "./test-data-jss"
  }
}
